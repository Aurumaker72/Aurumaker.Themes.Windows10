            <SolidColorBrush x:Key="RadioButtonDisabledBackgroundBrush"
                             Color="{StaticResource RadioButtonDisabledBackgroundColor}" />
            <SolidColorBrush x:Key="RadioButtonDisabledBorderBrush"
                             Color="{StaticResource RadioButtonDisabledBorderColor}" />

            <Color x:Key="EditFrameNormalBackgroundColor">#FFFFFF</Color>
            <Color x:Key="EditFrameNormalBorderColor">#7A7A7A</Color>
            <Color x:Key="EditFrameHoveredBackgroundColor">#FFFFFF</Color>
            <Color x:Key="EditFrameHoveredBorderColor">#171717</Color>
            <Color x:Key="EditFrameActiveBackgroundColor">#FFFFFF</Color>
            <Color x:Key="EditFrameActiveBorderColor">#0078D7</Color>
            <Color x:Key="EditFrameDisabledBackgroundColor">#FFFFFF</Color>
            <Color x:Key="EditFrameDisabledBorderColor">#CCCCCC</Color>

            <SolidColorBrush x:Key="EditFrameNormalBackgroundBrush"
                             Color="{StaticResource EditFrameNormalBackgroundColor}" />
            <SolidColorBrush x:Key="EditFrameNormalBorderBrush" Color="{StaticResource EditFrameNormalBorderColor}" />
            <SolidColorBrush x:Key="EditFrameHoveredBackgroundBrush"
                             Color="{StaticResource EditFrameHoveredBackgroundColor}" />
            <SolidColorBrush x:Key="EditFrameHoveredBorderBrush" Color="{StaticResource EditFrameHoveredBorderColor}" />
            <SolidColorBrush x:Key="EditFrameActiveBackgroundBrush"
                             Color="{StaticResource EditFrameActiveBackgroundColor}" />
            <SolidColorBrush x:Key="EditFrameActiveBorderBrush" Color="{StaticResource EditFrameActiveBorderColor}" />
            <SolidColorBrush x:Key="EditFrameDisabledBackgroundBrush"
                             Color="{StaticResource EditFrameDisabledBackgroundColor}" />
            <SolidColorBrush x:Key="EditFrameDisabledBorderBrush" Color="{StaticResource EditFrameDisabledBorderColor}" />

            <SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{StaticResource ThemeBackgroundColor}" />
            <SolidColorBrush x:Key="ThemeBorderLowBrush" Color="{StaticResource ThemeBorderLowColor}" />
            <SolidColorBrush x:Key="ThemeBorderMidBrush" Color="{StaticResource ThemeBorderMidColor}" />
            <SolidColorBrush x:Key="ThemeBorderHighBrush" Color="{StaticResource ThemeBorderHighColor}" />
            <SolidColorBrush x:Key="ThemeControlLowBrush" Color="{StaticResource ThemeControlLowColor}" />
            <SolidColorBrush x:Key="ThemeControlMidBrush" Color="{StaticResource ThemeControlMidColor}" />
