            <Color x:Key="RaisedFrameDisabledBorderColor">#BFBFBF</Color>

            <Color x:Key="RadioButtonNormalBackgroundColor">#FFFFFF</Color>
            <Color x:Key="RadioButtonNormalBorderColor">#333333</Color>
            <Color x:Key="RadioButtonHoveredBackgroundColor">#FFFFFF</Color>
            <Color x:Key="RadioButtonHoveredBorderColor">#0078D7</Color>
            <Color x:Key="RadioButtonActiveBackgroundColor">#FFFFFF</Color>
            <Color x:Key="RadioButtonActiveBorderColor">#333333</Color>
            <Color x:Key="RadioButtonDisabledBackgroundColor">#FFFFFF</Color>
            <Color x:Key="RadioButtonDisabledBorderColor">#CCCCCC</Color>
            
            <Color x:Key="DisabledForegroundColor">#838383</Color>
            <Color x:Key="DisabledForegroundColor2">#6D6D6D</Color>

            <SolidColorBrush x:Key="DisabledForegroundBrush"
                             Color="{StaticResource DisabledForegroundColor}" />
