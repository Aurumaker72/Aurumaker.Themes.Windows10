                             Color="{StaticResource DisabledForegroundColor2}" />

            <SolidColorBrush x:Key="RaisedFrameNormalBackgroundBrush"
                             Color="{StaticResource RaisedFrameNormalBackgroundColor}" />
            <SolidColorBrush x:Key="RaisedFrameNormalBorderBrush" Color="{StaticResource RaisedFrameNormalBorderColor}" />
            <SolidColorBrush x:Key="RaisedFrameHoveredBackgroundBrush"
                             Color="{StaticResource RaisedFrameHoveredBackgroundColor}" />
            <SolidColorBrush x:Key="RaisedFrameHoveredBorderBrush"
                             Color="{StaticResource RaisedFrameHoveredBorderColor}" />
            <SolidColorBrush x:Key="RaisedFrameActiveBackgroundBrush"
                             Color="{StaticResource RaisedFrameActiveBackgroundColor}" />
            <SolidColorBrush x:Key="RaisedFrameActiveBorderBrush" Color="{StaticResource RaisedFrameActiveBorderColor}" />
            <SolidColorBrush x:Key="RaisedFrameDisabledBackgroundBrush"
                             Color="{StaticResource RaisedFrameDisabledBackgroundColor}" />
